  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^5.10.0",
    "axios": "^0.18.1",
    "browserslist": "^4.8.0",
    "caniuse-lite": "^1.0.30001015",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "downshift": "^3.4.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "html-react-parser": "^0.7.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.15",
    "match-sorter": "^3.1.1",
    "pm2": "^3.5.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^2.1.8",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "production": "pm2-runtime server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage",
    "lint": "prettier --write **/*.{js,ts,tsx,scss,html,json} && tslint --fix -p ./tsconfig.json"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/faker": "^4.1.8",
    "@types/googlemaps": "^3.38.1",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/match-sorter": "^2.3.0",
    "@types/node": "^11.15.3",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "babel-polyfill": "^6.26.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.2.1",
    "react-app-polyfill": "^0.2.2",
    "react-testing-library": "^6.1.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,scss,html,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!/**/*.d.ts",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!src/utils/mixins.ts",
      "!src/utils/api.ts",
      "!src/types.ts",
      "!src/components/Icon.tsx",
      "!src/pages/findLiaison/LiaisonMap.tsx",
      "!src/components/Elements.tsx",
      "!src/components/Icons.tsx",
      "!src/pages/Icons.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 80,
        "functions": 95,
        "lines": 95
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
