      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!/**/*.d.ts",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!src/utils/mixins.ts",
      "!src/utils/api.ts",
      "!src/types.ts",
      "!src/components/Icon.tsx",
      "!src/pages/findLiaison/LiaisonMap.tsx",
      "!src/components/Elements.tsx",
      "!src/components/Icons.tsx",
      "!src/pages/Icons.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 80,
        "functions": 95,
        "lines": 95
      }
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
