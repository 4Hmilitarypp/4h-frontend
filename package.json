  "private": false,
  "proxy": "https://api.cms.4h.wendte.tech",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^6.1.0",
    "@types/react-big-calendar": "^0.30.0",
    "axios": "^0.21.1",
    "core-js": "^3.2.1",
    "date-fns": "^2.9.0",
    "dotenv": "^8.0.0",
    "downshift": "^6.1.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "html-react-parser": "^1.2.4",
    "lodash": "^4.17.15",
    "match-sorter": "^6.2.0",
    "moment": "^2.29.2",
    "pm2": "^4.5.4",
    "react": "^17.0.1",
    "react-big-calendar": "^0.31.0",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.2",
    "react-spring": "^8.0.27",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "production": "pm2-runtime server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage",
    "lint": "prettier --write **/*.{js,ts,tsx,scss,html,json} && eslint --fix src/**/*.{js,ts,tsx}"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/date-fns": "^2.6.0",
    "@types/faker": "^5.1.6",
    "@types/googlemaps": "^3.37.7",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.141",
    "@types/match-sorter": "^6.0.0",
    "@types/node": "^14.14.25",
    "@types/reach__router": "^1.2.5",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "faker": "^5.3.1",
    "husky": "^4.3.8",
    "jest": "26.6.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-app-polyfill": "^2.0.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,scss,html,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!/**/*.d.ts",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!src/utils/mixins.ts",
      "!src/utils/api.ts",
      "!src/types.ts",
      "!src/components/Icon.tsx",
      "!src/pages/findLiaison/LiaisonMap.tsx",
      "!src/components/Elements.tsx",
      "!src/components/Icons.tsx",
      "!src/pages/Icons.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 80,
        "functions": 95,
        "lines": 95
      }
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
