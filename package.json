{
  "name": "4h-military-partnership",
  "version": "0.8.1",
  "private": false,
  "proxy": "https://api.cms.4h.wendte.tech/",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^5.5.0",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "date-fns": "^2.9.0",
    "dotenv": "^8.0.0",
    "downshift": "^3.3.4",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "html-react-parser": "^0.9.1",
    "lodash": "^4.17.15",
    "match-sorter": "^4.0.0",
    "pm2": "^3.5.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "^3.1.2",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "production": "pm2-runtime server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage",
    "lint": "prettier --write **/*.{js,ts,tsx,scss,html,json} && eslint --fix src/**/*.{js,ts,tsx}"
  },
  "devDependencies": {
    "@testing-library/react": "^9.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/faker": "^4.1.5",
    "@types/googlemaps": "^3.37.7",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.141",
    "@types/match-sorter": "^2.3.0",
    "@types/node": "^12.7.8",
    "@types/reach__router": "^1.2.5",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "faker": "^4.1.0",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "react-app-polyfill": "^1.0.3",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,scss,html,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!/**/*.d.ts",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!src/utils/mixins.ts",
      "!src/utils/api.ts",
      "!src/types.ts",
      "!src/components/Icon.tsx",
      "!src/pages/findLiaison/LiaisonMap.tsx",
      "!src/components/Elements.tsx",
      "!src/components/Icons.tsx",
      "!src/pages/Icons.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 80,
        "functions": 95,
        "lines": 95
      }
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
